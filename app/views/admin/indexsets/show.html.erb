<% if  notice %>
  <div class="alert alert-success">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <h4>操作提示</h4>
    <%= notice %>
  </div>
<% end %>

<p>
  <b>模块名称:</b>
  <%= @indexset.module %>
</p>

<p>
  <b>标题:</b>
  <%= @indexset.title %>
</p>

<p>
  <b>图片:</b>
  <%= image_tag(@indexset.picture) %>
</p>

<p>
  <b>链接地址:</b>
  <%= raw @indexset.url %>
</p>


<%= link_to '编辑', edit_admin_indexset_path(@indexset) %> |
<%= link_to '返回', admin_indexsets_path %>